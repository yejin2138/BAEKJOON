
#include<stdio.h>
int main()
{
    int a = 0, b = 0,c=0,d=0;
    int result1, result2, result3, result4;
    scanf("%d", &b);
    scanf("%d", &a);
    result1 = b * (a%10);
    result2 = b*((a%100)/10);
    result3 = b*(a/100);
    result4 = result1 + result2*10 + result3*100;

    printf("%d\n%d\n%d\n%d", result1, result2, result3, result4);
}

