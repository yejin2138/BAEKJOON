
#include<stdio.h>
int main()
{
    int a = 0, b = 0;
    int result1, result2, result3, result4,result5;
    scanf("%d %d", &a, &b);
    result1 = a + b;
    result2 = a - b;
    result3 = a  *b;
    result4 = a / b;
    result5 = a % b;
    printf("%d\n%d\n%d\n%d\n%d", result1, result2, result3, result4, result5);
}

