
#define _CRT_SECURE_NO_WARNINGS
#include<stdio.h>

int main() {
	int n_s1, n_s2,s1,s2;
	
	scanf("%d %d", &s1,&s2);
	n_s1 = (s1 / 100) + (s1 / 10 % 10 * 10) + (s1 % 10 * 100);
	n_s2 = (s2 / 100) + (s2 / 10 % 10 * 10) + (s2 % 10 * 100);

	if (n_s1 < n_s2) {
		printf("%d\n", n_s2);
	}
	else {
		printf("%d\n", n_s1);
	}
}

